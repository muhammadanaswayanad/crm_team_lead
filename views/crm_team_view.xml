<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="view_crm_team_lead_form" model="ir.ui.view">
    <field name="name">crm.team.lead.form</field>
    <field name="model">crm.team</field>
    <field name="arch" type="xml">
        <form string="Team Lead">
            <sheet>
                <group>
                    <field name="name"/>
                    <field name="member_ids" widget="many2many_tags"/>
                </group>
            </sheet>
        </form>
    </field>
</record>

<record id="view_crm_team_lead_tree" model="ir.ui.view">
    <field name="name">crm.team.lead.tree</field>
    <field name="model">crm.team</field>
    <field name="arch" type="xml">
        <tree string="Team Leads">
            <field name="name"/>
            <field name="member_ids"/>
        </tree>
    </field>
</record>

<record id="action_crm_team_lead" model="ir.actions.act_window">
    <field name="name">Team Leads</field>
    <field name="res_model">crm.team</field>
    <field name="view_mode">tree,form</field>
</record>

<menuitem id="menu_crm_team_lead" name="Team Leads" parent="crm.crm_menu_root" action="action_crm_team_lead" sequence="10"/>
</odoo>